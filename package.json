    "scripts": {
        "postinstall": "bower install",
        "prestart": "npm install",
        "start": "http-server -a localhost -p 8000 -c-1",
        "pretest": "npm install && mkdirp test_out",
        "test": "karma start karma/karma.conf.js",
        "pretest-single-run": "mkdirp test_out",
        "test-single-run": "karma start build/karma.conf.js  --single-run",
        "bower-update": "bower update",
        "bower": "bower",
        "ngdocs": "grunt ngdocs",
        "grunt": "grunt",
        "preupdate-webdriver": "npm install",
        "update-webdriver": "webdriver-manager update",
        "update-webdriver-ie": "webdriver-manager update --ie",
        "preprotractor": "npm run update-webdriver",
        "protractor": "protractor build/protractor.conf.js",
        "update-index-async": "node -e \"require('shelljs/global'); sed('-i', /\\/\\/@@NG_LOADER_START@@[\\s\\S]*\\/\\/@@NG_LOADER_END@@/, '//@@NG_LOADER_START@@\\n' + cat('lib/angular-loader/angular-loader.min.js') + '\\n//@@NG_LOADER_END@@', 'src/template-async.html');\""
    }
